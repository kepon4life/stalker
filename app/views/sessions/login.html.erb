<% @title="Login" %>


<%= form_tag({:action => 'login_attempt'}, :class => 'form-signin') do %>
	<h2 class="form-signin-heading">Log in</h2>
	<p>Username :</br> <%= text_field_tag(:username) %></p> 
	<p>Password:</br> <%= password_field_tag :login_password %></p> 
	<%= submit_tag("Log In") %>
<% end %>
